
view,
label,
swiper-item,
scroll-view {
  display:flex;
  flex-direction:column;
  flex-shrink: 0;
  flex-grow: 0;
  flex-basis: auto;
  align-items: stretch;
  align-content: flex-start;
}
view,
image,
input,
scroll-view,
swiper,
swiper-item,
text,
textarea,
video {
  position: relative;
  border: 0px solid #000000;
  box-sizing: border-box;
}
swiper-item {
  position: absolute;
}
button {
  margin: 0;
}


page {
	width: 100%;
	min-height: 100%;
	display: flex;
	/* padding-top: 80px; */
}
.page {
	flex: 1;
}
.header {
	position: relative;
	width: 100vw;
	/* height: 40%; */
	height: 260px;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	background-color: #fff;
	border-bottom-left-radius: 50vw;
	border-bottom-right-radius: 50vw;
	box-shadow: 0 2px 10px 0px  rgba(25,107,166,0.1);
}
.headerIcon {
	position: absolute;
	top: 80px;
	width: 20px;
	height: 20px;
}
.back {
	left: 30px;
}
.avatar {
	margin-top: 60px;
	border-radius: 50%;
	width: 70px;
	height: 70px;
	background-image: url(data:image/png;base64,);
	background-size: cover;
}
.name {
	margin-top: 10px;
	letter-spacing: 5rpx;
	font-size: 20px;
	color: #616161;
	font-weight: bold;
}
.main-content {
	width: 90vw;
	margin: 0 auto;
	height: 60%;
	padding: 40px;
	padding-top: 10px;
	/* padding-top: 20px; */
	display: flex;
}
.list {
	flex: 1;
	display: flex;
	height: 100%;
	flex-direction: column;
}
.list-cell {
	flex: 1;
	display: flex;
	
	align-items: center;
}
.list-item {
	margin-top: 4px;
	margin-bottom: 4px;
	width: 100%;
	/* height: 40px; */
	padding: 15px;
	background-color: #fff;
	border-radius: 15px;
	box-shadow: 0 2px 10px 0px  rgba(25,107,166,0.1);
	display: flex;	
	/* justify-content: space-around; */
	align-items: center;
	flex-direction: row;
	position: relative;
}
.itemIcon {
	width: 20px;
	height: 20px;
	margin-right: 60px;
}
.itemTitle {
	position: absolute;
	left: 50%;
	font-size: 16px;
	color: #6B400D;
	font-weight: 550;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
}

